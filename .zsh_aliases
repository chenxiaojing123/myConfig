# Tor Command Line Tools by @gravmatt (gravmatt.com)
alias myip='curl -s https://api.ipify.org/'
alias myiplookup='ip2cc $(curl -s https://api.ipify.org/)'
alias mytorip='curl -s --socks5 0.0.0.0:9050 https://api.ipify.org/'
